Q1 Solution
def square_root_generator(limit):
    n = 1
    while n <= limit:
        yield n ** 0.5
        n += 1

# Example usage:
limit = 5
generator = square_root_generator(limit)

total =[]
for sqrt_value in generator:

    total.append(sqrt_value)
    print(sqrt_value)

print("Total",sum(total))
